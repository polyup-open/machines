{
  "compilerOptions" : {
      "lib": [ "es6", "es2016", "dom" ],
      "target": "es5",
      "outDir": "lib",
      "noImplicitAny": true,
      "sourceMap": true ,
      "allowJs": true,
      "baseUrl": ".",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "downlevelIteration": true,
      "resolveJsonModule": true
  },

  "include": [
    "src/**/*"
  ]
}
